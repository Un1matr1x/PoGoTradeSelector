{% extends 'base.html.twig' %}

{% block body %}
    <div class="row">
    {% for i in 1..898 %}
            <div class=" col-xs-6 col-sm-6 col-md-4 col-lg-3 col-xl-3 col-xxl-2 col-2 text-center">
                <img src="https://assets.pokemon.com/assets/cms2/img/pokedex/detail/{{ "%03d"|format(i) }}.png" class="img-thumbnail border-0">
            </div>
    {% endfor %}
    </div>
{% endblock %}
